# Startup Routine
AddCharset utf-8 .css
AddCharset utf-8 .js
AddCharset utf-8 .txt
AddCharset utf-8 .xml
AddDefaultCharset utf-8
AddOutputFilterByType DEFLATE application/javascript application/pdf application/rss+xml application/xml font/eot font/otf font/ttf image/svg+xml image/vnd.microsoft.icon image/x-icon text/css text/html text/javascript text/plain text/xml
AddType application/xml .xml
AddType font/eot .eot
AddType font/otf .otf
AddType font/ttf .ttf
AddType font/woff .woff
AddType font/woff2 .woff2
CheckSpelling On
ContentDigest On
DefaultLanguage de
ExpiresActive On
ExpiresByType application/javascript A129600
ExpiresByType application/rss+xml A60
ExpiresByType application/xml A60
ExpiresByType font/eot A604800
ExpiresByType font/otf A604800
ExpiresByType font/ttf A604800
ExpiresByType font/woff A604800
ExpiresByType font/woff2 A604800
ExpiresByType image/gif A604800
ExpiresByType image/jpeg A604800
ExpiresByType image/png A604800
ExpiresByType image/svg+xml A604800
ExpiresByType image/vnd.microsoft.icon A604800
ExpiresByType image/x-icon A604800
ExpiresByType text/css A129600
ExpiresByType text/html A60
ExpiresByType text/javascript A129600
ExpiresByType text/plain A60
ExpiresByType text/xml A60
FileETag All
Header always edit Set-Cookie (.*) "$1; SameSite=Lax; Secure"
Header always set X-Content-Type-Options nosniff
Header always set X-Frame-Options SAMEORIGIN

<FilesMatch "\.(html|php)$">
	Header always set Content-Security-Policy "default-src 'self'; font-src 'self' fonts.gstatic.com; img-src 'self' data: maps.googleapis.com maps.gstatic.com; script-src 'self' 'unsafe-inline' maps.googleapis.com; style-src 'self' 'unsafe-inline' fonts.googleapis.com"
	Header always set Feature-Policy "camera 'none'; microphone 'none'; payment 'none'"
	Header always set Referrer-Policy "strict-origin-when-cross-origin"
	Header always set X-XSS-Protection "1; mode=block"
</FilesMatch>

<FilesMatch "index\.xml$">
	ForceType application/rss+xml
</FilesMatch>

# Course Calibration
<IfModule mod_rewrite.c>
	RewriteEngine On
	RewriteCond %{HTTPS} !=on [OR]
	RewriteCond %{HTTP_HOST} !^hhkaffee\.com$ [NC]
	RewriteRule ^ https://hhkaffee.com%{REQUEST_URI} [R=301,L]
</IfModule>

